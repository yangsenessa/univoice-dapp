{
  "name": "univoice-dapp-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "setup": "npm i && dfx canister create univoice-dapp-backend && dfx generate univoice-dapp-backend && dfx deploy",
    "start": "vite --port 5000",
    "dev": "vite serve --config vite.dev.config.js --no-clear",
    "prebuild": "dfx generate",
    "build": "tsc && vite build",
    "format": "prettier --write \"src/**/*.{json,js,jsx,ts,tsx,css,scss}\"",
    "generate-certs": "chmod +x ./generate-certificates.sh && ./generate-certificates.sh",
    "start:https": "vite --port 5000",
    "serve": "node server/index.js"
  },
  "dependencies": {
    "@dfinity/agent": "^1.4.0",
    "@dfinity/candid": "^1.4.0",
    "@dfinity/identity": "^1.4.0",
    "@dfinity/principal": "^1.4.0",
    "@ldclabs/ic_oss_ts": "^1.0.1",
    "@lottiefiles/dotlottie-react": "^0.13.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-media-recorder-2": "1.6.23",
    "react-router-dom": "^7.1.1",
    "simple-zustand-devtools": "^1.1.0",
    "zustand": "^5.0.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@types/node": "^22.14.1",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@vitejs/plugin-react": "^3.1.0",
    "dotenv": "^16.3.1",
    "sass": "^1.63.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.3",
    "vite": "^4.5.9",
    "vite-plugin-environment": "^1.1.3"
  }
}
